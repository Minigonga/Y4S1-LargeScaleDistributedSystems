{
  "name": "shopping-list-server",
  "version": "1.0.0",
  "private": true,
  "description": "Backend for shopping list project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpile-only src/index.ts",
    "cloud": "npx concurrently \"npx ts-node src/api/coordinatorLauncher.ts\" \"npx ts-node src/api/cloudLauncher.ts\"",
    "coordinator": "npx ts-node src/api/coordinatorLauncher.ts",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "zeromq": "^6.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.13",
    "@types/sqlite3": "^5.1.0",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
